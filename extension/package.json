{
  "name": "Altruisto.com",
  "version": "3.0.3",
  "description": "Browser extension that allows users to raise money for charities through online shoppings with no extra costs",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "build": "rm -rf build && webpack",
    "build:panel": "rm -rf build/.panel && webpack --config-name panel",
    "quickbuild": "rm -rf build && webpack --prototype",
    "quickbuild:staging": "rm -rf build && webpack --prototype --define BASE_URL='\"https://altruisto-backend-staging.herokuapp.com\"'",
    "quickbuild:production": "rm -rf build && webpack --prototype --define BASE_URL='\"https://api.altruisto.com\"'",
    "prototype:panel": "webpack-dev-server --config-name panel --mode development --prototype --inline --hot --open",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altruisto/altruisto.git"
  },
  "author": "Daniel Wyrzykowski",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/altruisto/altruisto/issues"
  },
  "homepage": "https://altruisto.com",
  "dependencies": {
    "@material-ui/core": "4.3.0",
    "axios": "0.21.1",
    "copy-to-clipboard": "3.2.0",
    "formik": "1.5.8",
    "loaders.css": "0.1.2",
    "notistack": "0.8.9",
    "pure-react-carousel": "1.21.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-swipeable-views": "0.13.3"
  },
  "devDependencies": {
    "@types/chrome": "0.0.87",
    "@types/jest": "25.1.1",
    "@types/psl": "1.1.0",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "before-build-webpack": "0.2.9",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.0.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "file-loader": "4.1.0",
    "handlebars": "4.7.7",
    "handlebars-loader": "1.7.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.18.2",
    "psl": "1.7.0",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "webextension-polyfill": "0.4.0",
    "webextension-polyfill-ts": "0.12.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-middleware": "3.7.0",
    "webpack-dev-server": "3.7.2",
    "webpack-extension-reloader": "1.1.0",
    "webpack-hot-middleware": "2.25.0",
    "zip-webpack-plugin": "3.0.0"
  }
}
